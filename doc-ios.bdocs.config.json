{
	"doxyfilePath": "./Doxyfile-iOS",
	"doxygenOutputPath": "./docs-iOS",
	"websiteRootPath": "~/Projects/icedcoffee-meta/website/",
	"relativeOutputPath": "content/reference/ios/",

	"executeDoxygen": true,
	"executeNanoc": true,
	"cleanIntermediates": false,

	"mainTitle": "Icedcoffee iOS API Reference",
	"version": "0.7",
	
	"classPrefix": "IC",
	"macroPrefix": "IC_",
	
	"generateYAML": true,
	"documentIvars": false,
	
	"nameMap": {
		"anonEnum": {
			"ICHitTest(.*)": "ICHitTestResult",
			"ICControlEvent(.*)": "ICControlEvents",
			"ICAbstractControlEvent(.*)": "ICAbstractControlEvents",
			"IC(.*)Version(.*)": "ICOSVersion"			
		}
	}
}